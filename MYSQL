CREATE TABLE Employees
(
EmpID int NOT NULL,
EmpName varchar(255),
EmpAddress varchar(255),
EmpDOB varchar(11),
EmpMobile int,
PRIMARY KEY (EmpID)
);

INSERT INTO Employees (EmpID, EmpName, EmpAddress, EmpDOB, EmpMobile)
VALUES ('1001','Tom','21 Old Street, UK','11-11-2004','5544789631');

DELETE FROM Employees WHERE EmpID='1001';

UPDATE Employees
SET EmpName='Alfred'
WHERE EmpID=1002;

SELECT * FROM Employees
WHERE EmpName LIKE 'T%';